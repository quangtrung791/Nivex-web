generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")       // URL có ?pgbouncer=true
  directUrl = env("POSTGRES_URL_NON_POOLING")  // Dùng cho migrate
}

model test1 {
  id   Int     @id @default(autoincrement())
  name String?
  
  @@map("test1")
}
