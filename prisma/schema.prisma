generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL_VERCEL")       // URL cรณ ?pgbouncer=true
}

model test1 {
  id   Int     @id @default(autoincrement())
  name String?
  
  @@map("test1")
}
